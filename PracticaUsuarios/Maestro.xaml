<UserControl x:Class="PracticaUsuarios.Maestro"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PracticaUsuarios"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <WrapPanel Orientation="Vertical" Background="White" Margin="0,0,-205,0">
        <TextBlock Name="lblNombreMaestro">Nombre:</TextBlock>
        <TextBox Width="200" Name="txtNombreMaestro"></TextBox>
        <TextBlock Name="lblTeldefonoMaestro">Telefono:</TextBlock>
        <TextBox Width="200" Name="txtTelefonoMaestro"></TextBox>
        <TextBlock Name="lblCedula">Cedula Profesional:</TextBlock>
        <TextBox Width="200" Name="txtCedula"></TextBox>
        <TextBlock Name="lblCarreraMaestro">Carrera estudiada:</TextBlock>
        <TextBox Width="200" Name="txtCarrreraMaestro"></TextBox>
    </WrapPanel>
</UserControl>
