<UserControl x:Class="PracticaUsuarios.Maestro"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PracticaUsuarios"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <WrapPanel Orientation="Vertical" Background="White" Margin="0,0,-205,0">
        <TextBlock>Nombre:</TextBlock>
        <TextBox Width="200"></TextBox>
        <TextBlock>Telefono:</TextBlock>
        <TextBox Width="200"></TextBox>
        <TextBlock>Cedula Profesional:</TextBlock>
        <TextBox Width="200"></TextBox>
        <TextBlock>Carrera estudiada:</TextBlock>
        <TextBox Width="200"></TextBox>
        <Button Width="100" Margin="150,0,0,0">Guardar</Button>
    </WrapPanel>
</UserControl>
