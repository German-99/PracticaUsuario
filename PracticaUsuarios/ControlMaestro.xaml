<UserControl x:Class="PracticaUsuarios.ControlMaestro"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PracticaUsuarios"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <WrapPanel Orientation="Vertical" Background="White" Margin="0,0,-205,0">
        <TextBlock Name="lblNombreM" FontWeight="Bold">Nombre:</TextBlock>
        <TextBox Width="200" Name="txtNombreM"></TextBox>
        <TextBlock Name="lblTelefonoM" FontWeight="Bold">Telefono:</TextBlock>
        <TextBox Width="200" Name="txtTelefonoM"></TextBox>
        <TextBlock Name="lblCedulaM" FontWeight="Bold">Cedula Profesional:</TextBlock>
        <TextBox Width="200" Name="txtCedulam"></TextBox>
        <TextBlock Name="lblCarreraM" FontWeight="Bold">Carrera estudiada:</TextBlock>
        <TextBox Width="200" Name="txtCarreraM"></TextBox>
    </WrapPanel>
</UserControl>
